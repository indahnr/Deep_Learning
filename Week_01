print('Contoh Seleksi kelulusan')
siswa = input('Nama: ')
nilai = int(input('Nilai: '))
if nilai>70:
  print (siswa,'Dinyatakan Lulus')
elif nilai==70:
  print (siswa,'Akan Mendapatkan Kelas Tambahan Agar Dapat Dinyatakan Lulus')
else:
  print (siswa,'Dinyatakan tidak Lulus')
